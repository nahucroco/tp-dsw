	},
	"homepage": "https://github.com/nahucroco/tp-dsw#readme",
	"dependencies": {
		"@mikro-orm/core": "^6.5.8",
		"@mikro-orm/mysql": "^6.5.8",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"express": "^5.1.0",
		"reflect-metadata": "^0.2.2",
		"zod": "4.1.5"
	},
	"keywords": [],
