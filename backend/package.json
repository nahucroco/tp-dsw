{
	"name": "backend",
	"version": "1.0.0",
	"description": "proyecto biblioteca",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"test": "ts-node-dev --respawn --transpile-only src/models/Book.ts",
		"dev": "ts-node-dev --respawn --transpile-only src/index.ts",
		"format": "npx @biomejs/biome format --write ./src",
		"start:dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/index.js\"",
		"lint": "npx @biomejs/biome lint --write ./src",
		"build": "tsc -p tsconfig.json",
		"schema:update": "mikro-orm schema:update --run --config mikro-orm.config.js",
		"schema:refresh": "mikro-orm schema:refresh --seed --config mikro-orm.config.js",
		"migrate:gen": "mikro-orm migration:generate --config mikro-orm.config.js",
		"migrate:up": "mikro-orm migration:up --config mikro-orm.config.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nahucroco/tp-dsw.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/nahucroco/tp-dsw/issues"
	},
	"homepage": "https://github.com/nahucroco/tp-dsw#readme",
	"dependencies": {
		"@mikro-orm/core": "6.5.9",
		"@mikro-orm/knex": "6.5.9",
		"@mikro-orm/migrations": "6.5.9",
		"@mikro-orm/mysql": "6.5.9",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"reflect-metadata": "^0.2.2",
		"zod": "4.1.5"
	},
	"keywords": [],
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@mikro-orm/cli": "6.5.9",
		"@types/bcrypt": "^6.0.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/node": "^24.1.0",
		"dotenv": "^17.2.3",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-watch": "^7.2.0",
		"typescript": "^5.9.3"
	}
}